<webscript>
  <shortname>AJAX Auto Suggest for Space Names</shortname>
  <description>Web Script that returns a list of spaces for the AJAX auto-suggest</description>
  <url>/zk-filesystemsuggestion/suggest/spaces?query={query}&amp;repositoryRootPath={repositoryRootPath}&amp;includeSites=${includeSites}</url>
  <authentication>user</authentication>
  <format default="json">any</format>
  <family>zk-filesystemsuggestiont</family>
  <cache>
    <never>true</never>
  </cache>
</webscript>